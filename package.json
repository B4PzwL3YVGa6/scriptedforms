  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile && yarn run sass && webpack",
    "copy": "cp -r ./src ./build ",
    "sass": "node-sass ./src/theme.scss ./build/theme.css",
    "compile": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf build",
    "prepack": "yarn run build"
  },
  "files": [
    "build/**/*.{d.ts,eot,gif,html,css,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
